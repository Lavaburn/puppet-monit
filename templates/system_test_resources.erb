---
<% if @system_loadavg_1min %>
 - type      : 'loadavg(1min)'
   operator  : '>'
   value     : <%= @system_loadavg_1min %>
<% end %>
<% if @system_loadavg_5min %>
 - type      : 'loadavg(5min)'
   operator  : '>'
   value     : <%= @system_loadavg_5min %>
<% end %>
<% if @system_loadavg_15min %>
 - type      : 'loadavg(15min)'
   operator  : '>'
   value     : <%= @system_loadavg_15min %>
<% end %>
<% if @system_cpu_user %>
 - type      : 'cpu(user)'
   operator  : '>'
   value     : <%= @system_cpu_user %>
<% end %>
<% if @system_cpu_system %>
 - type      : 'cpu(system)'
   operator  : '>'
   value     : <%= @system_cpu_system %>
<% end %>
<% if @system_cpu_wait %>
 - type      : 'cpu(wait)'
   operator  : '>'
   value     : <%= @system_cpu_wait %>
<% end %>
<% if @system_memory %>
 - type      : 'memory'
   operator  : '>'
   value     : <%= @system_memory %>
<% end %>
<% if @system_swap %>
 - type      : 'swap'
   operator  : '>'
   value     : <%= @system_swap %>
<% end %>
